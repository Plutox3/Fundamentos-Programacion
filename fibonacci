
public class Fibonacci {

    
    public static void main(String[] args) {
        int n1 = 0;
        int n2 = 1;
        int total = n1+n2;
        
        System.out.println(total);
        
        int numero = Integer.parseInt( JOptionPane.showInputDialog("ingresa un numero culaquiera"));
        
        for(int contador = 0;contador<=numero-2;contador++){
        n1=n2;
        n2 = total;
        total=n1+n2;
        System.out.println(total);
        }
        
       
    }
    
}
